<body>
<div id="cont">
	<div id="headBar">
		Rule Setter
	</div>
	<div style="clear:both"></div>
	<div class="row">
		<form class="col s12">
		    <div class="row">
		    	<div class="input-field col s12">
	        		<label>Room</label>
					  <select style="margin: 40px 0 -30px 0" class="browser-default">
					    <option value="" disabled selected>Choose your option</option>
					    <option value="1">Option 1</option>
					    <option value="2">Option 2</option>
					    <option value="3">Option 3</option>
					  </select>
				</div>
		    </div>
		    <div class="row">
		    	<div class="input-field col s12">
	        		<label>Checks:</label>
					  <select style="margin: 40px 0 -10px 0" class="browser-default">
					    <option value="" disabled selected>Choose your option</option>
					    <option value="time">Time</option>
				  		<option value="light">Light</option>
				  		<option value="temp">Temperature</option>
					  </select>
				</div>
		    </div>
		    <div class="row">
		        <div class="input-field col s6">
		        	<input id="min" type="number" class="validate">
		        	<label for="min">Min</label>
		        </div>
		        <div class="input-field col s6">
		        	<input id="max" type="number" class="validate">
		        	<label for="max">Max</label>
		        </div>
		    </div>
		    <div class="row">
		        <div class="input-field col s12">
		        	<input id="cmd" type="text" class="validate">
		        	<label for="cmd">Command</label>
		        </div>
		    </div>
		    <div class="row">
		        <div class="input-field col s8">

		        </div>
		        <div class="input-field col s4">
		        	<a class="waves-effect waves-light btn">Save</a>
		        </div>
		    </div>
		</form>
	</div>
</div>
